{
  "name": "elearning-system",
  "version": "",
  "license": "ISC",
  "description": "",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project tsconfig.compile.json && tsc-alias",
    "barrels": "barrelsby --config .barrelsby.json -q",
    "start:fe": "yarn barrels && cross-env NODE_ENV=development vite --host",
    "start:be": "yarn barrels && cross-env NODE_ENV=development tsnd --ignore-watch node_modules --respawn --transpile-only --max-old-space-size=2048 -r tsconfig-paths/register backend/index.ts",
    "lint": "eslint '**/*.{tsx,ts,js}'",
    "lint:fix": "eslint '**/*.{ts,js}' --fix",
    "format": "prettier '**/*.{tsx,ts,js,json,md,yml,yaml}' --write",
    "clean:git": "git branch --merged >/tmp/merged-branches && nano /tmp/merged-branches && xargs git branch -D </tmp/merged-branches && git fetch --prune --all",
    "prepare": "husky install"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@material-tailwind/react": "^2.1.5",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "envalid": "^7.3.1",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongodb": "^6.5.0",
    "mongoose": "^8.2.3",
    "mongoose-auto-increment": "^5.0.1",
    "mqtt": "^5.5.0",
    "nodemailer": "^6.9.13",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.20.1",
    "react-toastify": "^9.1.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.4.8",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "barrelsby": "^2.8.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "pino-pretty": "^10.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.0.1",
    "tailwindcss": "^3.3.6",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.7",
    "vite-tsconfig-paths": "^4.2.2"
  }
}
